{
    "name": "@philtzjp/atac",
    "version": "0.4.4",
    "description": "ATAC (Any To Any Connectivity) - Discord Bot Core Framework",
    "type": "module",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "types": "./dist/index.d.ts"
        },
        "./voice": {
            "import": "./dist/voice/index.js",
            "types": "./dist/voice/index.d.ts"
        }
    },
    "files": [
        "dist",
        "README.md",
        "LICENSE"
    ],
    "homepage": "https://atac.one",
    "author": "Arata Ouchi (Philtz)",
    "license": "MPL-2.0",
    "repository": {
        "type": "git",
        "url": "https://github.com/philtzjp/atac"
    },
    "publishConfig": {
        "access": "public",
        "registry": "https://registry.npmjs.org"
    },
    "scripts": {
        "build": "tsc",
        "lint": "eslint src/**/*.ts",
        "lint:fix": "eslint src/**/*.ts --fix",
        "format": "prettier --write \"src/**/*.ts\"",
        "test": "jest",
        "test:watch": "jest --watch"
    },
    "engines": {
        "node": ">=24.0.0"
    },
    "keywords": [
        "discord",
        "bot",
        "framework",
        "atac"
    ],
    "peerDependencies": {
        "@discordjs/opus": "^0.10.0",
        "@discordjs/voice": "^0.18.0",
        "better-sqlite3": "^11.0.0 || ^12.0.0",
        "discord.js": "^14.0.0",
        "neo4j-driver": "^5.0.0 || ^6.0.0",
        "prism-media": "^1.3.5",
        "zod": "^3.0.0"
    },
    "peerDependenciesMeta": {
        "@discordjs/opus": {
            "optional": true
        },
        "@discordjs/voice": {
            "optional": true
        },
        "better-sqlite3": {
            "optional": true
        },
        "neo4j-driver": {
            "optional": true
        },
        "prism-media": {
            "optional": true
        }
    },
    "dependencies": {
        "dotenv": "^16.0.0"
    },
    "devDependencies": {
        "@discordjs/opus": "^0.10.0",
        "@discordjs/voice": "^0.18.0",
        "@types/better-sqlite3": "^7.6.13",
        "@types/jest": "^29.5.11",
        "@types/node": "^20.11.0",
        "@typescript-eslint/eslint-plugin": "^6.21.0",
        "@typescript-eslint/parser": "^6.21.0",
        "better-sqlite3": "^12.6.2",
        "discord.js": "^14.14.1",
        "eslint": "^8.56.0",
        "eslint-config-prettier": "^9.1.0",
        "jest": "^29.7.0",
        "neo4j-driver": "^6.0.1",
        "prettier": "^3.2.4",
        "prism-media": "^1.3.5",
        "ts-jest": "^29.1.2",
        "typescript": "^5.3.3",
        "zod": "^3.22.4"
    }
}
