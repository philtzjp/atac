{
    "name": "@philtzjp/atac",
    "version": "0.2.0",
    "description": "ATAC (Any To Any Connectivity) - Discord Bot Core Framework",
    "type": "module",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "types": "./dist/index.d.ts"
        }
    },
    "files": [
        "dist",
        "README.md",
        "LICENSE"
    ],
    "homepage": "https://atac.one",
    "author": "Arata Ouchi (Philtz)",
    "license": "MPL-2.0",
    "repository": {
        "type": "git",
        "url": "https://github.com/philtzjp/atac"
    },
    "publishConfig": {
        "access": "public",
        "registry": "https://registry.npmjs.org"
    },
    "scripts": {
        "build": "tsc",
        "lint": "eslint src/**/*.ts",
        "lint:fix": "eslint src/**/*.ts --fix",
        "format": "prettier --write \"src/**/*.ts\"",
        "test": "jest",
        "test:watch": "jest --watch"
    },
    "engines": {
        "node": ">=18.0.0"
    },
    "keywords": [
        "discord",
        "bot",
        "framework",
        "atac"
    ],
    "peerDependencies": {
        "discord.js": "^14.0.0",
        "zod": "^3.0.0"
    },
    "dependencies": {
        "dotenv": "^16.0.0",
        "@discordjs/voice": "^0.18.0",
        "@discordjs/opus": "^0.10.0",
        "prism-media": "^1.3.5"
    },
    "devDependencies": {
        "@types/node": "^20.11.0",
        "@typescript-eslint/eslint-plugin": "^6.21.0",
        "@typescript-eslint/parser": "^6.21.0",
        "discord.js": "^14.14.1",
        "eslint": "^8.56.0",
        "eslint-config-prettier": "^9.1.0",
        "prettier": "^3.2.4",
        "typescript": "^5.3.3",
        "zod": "^3.22.4",
        "jest": "^29.7.0",
        "@types/jest": "^29.5.11",
        "ts-jest": "^29.1.2"
    }
}
